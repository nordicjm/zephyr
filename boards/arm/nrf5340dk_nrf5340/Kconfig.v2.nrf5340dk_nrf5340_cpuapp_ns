	bool "nRF5340 DK nRF5340 Application MCU non-secure"
	default y
	select SOC_SERIES_NRF53X
	select SOC_NRF5340_CPUAPP_QKAA
	imply CONFIG_TRUSTED_EXECUTION_NONSECURE
