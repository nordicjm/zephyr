config BOARD_NRF5340_AUDIO_DK
	select SOC_NRF5340_CPUAPP_QKAA if "$(BOARD_IDENTIFIER)" = "/nrf5340/cpuapp"
	select SOC_NRF5340_CPUAPP_QKAA if "$(BOARD_IDENTIFIER)" = "/nrf5340/cpuapp/ns"
	select SOC_NRF5340_CPUNET_QKAA if "$(BOARD_IDENTIFIER)" = "/nrf5340/cpunet"
