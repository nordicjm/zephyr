# ESP32S3 Core board configuration
config BOARD_ESP32S3_LUATOS_CORE
	bool
	default y
	select SOC_SERIES_ESP32S3

config BOARD_ESP32S3_LUATOS_CORE_USB
	bool
	default y
	select SOC_SERIES_ESP32S3

choice SOC_PART_NUMBER
	default SOC_ESP32S3_MINI_N8
endchoice
