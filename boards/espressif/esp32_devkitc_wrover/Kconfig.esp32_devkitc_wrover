config BOARD_ESP32_DEVKITC_WROVER
	bool
	default y
	select SOC_SERIES_ESP32

config BOARD_ESP32_DEVKITC_WROVER_APPCPU
	bool
	default y
	select SOC_SERIES_ESP32

choice SOC_PART_NUMBER
	default SOC_ESP32_WROVER_E_N4R8
endchoice
